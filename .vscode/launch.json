    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "consoleName": "AssetFlow API",
            "cwd": "${workspaceFolder}\\src\\backend",
            "args": [
                "main:app",
                "--reload",
                "--workers", "4",
                "--limit-concurrency", "100"
            ],
            "jinja": true
        },
        {
            "name": "Simulator Y2021",
            "type": "debugpy",
            "request": "launch",
            "program": "simulator.py",
            "consoleName": "Simulator Y2021",
            "cwd": "${workspaceFolder}\\src\\simulator\\",
            "console": "integratedTerminal",
            "args": [ 
                "2021-01-01",
                "2021-12-29"           
            ],
            "jinja": true
        },
        {
